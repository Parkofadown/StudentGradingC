#include <stdio.h>

int main(void) {

	int grade;
	printf("Enter student grade:" );
	grade = getchar();
	printf("\nStudents grade is: ");
	putchar(grade);
    if (grade < 65){
    	printf("\nStudent grade is F");
    }
    else if (grade <= 69)
    {
    	printf("\nStudent grade is D");
    }
    else if (grade <= 79 ){
    	printf("\nStudent grade is C");
    }
    else if (grade <= 89){
    	printf("\nStudent grade is B");
    	}
    else if (grade <= 100){
    	printf("\nStudent grade is A");
    }
	return 0;
}
